{
    "compilerOptions": {
      "outDir": "./output",
      "module": "CommonJS",
      "target": "ESNext",
      "moduleResolution": "node",
      "skipLibCheck": true,
      "noEmitOnError": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "noImplicitThis": true
    },
    "include": ["./src/**/*"]
}